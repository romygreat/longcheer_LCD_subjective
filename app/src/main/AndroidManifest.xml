<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.longcheer_lcd_subjective">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Fullscreen_Color_Activity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen__color_"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".Fullscreen_brokendots_Activity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen_brokendots_"
            android:theme="@style/FullscreenTheme"></activity>
        <activity android:name=".SetPictrture_Activity"
            android:theme="@style/FullscreenTheme"
        >
            <intent-filter>
                <action android:name="android.com.hello" />
            </intent-filter>
        </activity>
    </application>

</manifest>